<div class="container">
        <header>
            <ul>
            <li><img class="logo" src="/images/logo.png" /></li>
            <li><a href="/">Browse</a></li>
            <li><a href="/create/cube">Add a Cube</a></li>
            <li><a href="/create/accessory">Add Accessory</a></li>
            <li><a href="/about">About</a></li>
            <li><a href="/auth/logout">Logout</a></li>
            <li><a href="/auth/login">Login</a></li>
            <li><a href="/auth/register">Register</a></li>
        </ul>
        </header>
        <main>
            <h1>Delete Cube</h1>
            <div class="form">
                <form action="#" method="POST">
                    
                    <label for="name">Name</label>
                    <input type="text" id="name" name="name" value="{{cube.name}}" disabled="true" />
                    <label for="description">Description</label>
                    <textarea id="description" name="description" disabled="true" />{{cube.description}}</textarea>
                    <label for="image">ImageUrl</label>
                    <input type="text" id="image" name="imageUrl" value="{{cube.imageUrl}}" disabled="true" />
                    <label for="difficulty">Difficulty</label>
                    <select id="difficulty" name="difficultyLevel" disabled="true" />
                    {{#each difficultyLevels}}
                        <option value="{{this.key}}" {{#if this.selected}} selected {{/if}}>{{this.label}}</option>
                    {{/each}}
                    </select>
                    <input type="submit" value="Delete">
                </form>
            </div>
        </main>
    </div>
    <footer>@Cubicle: Exercise for Express.js and Handlebars</footer>