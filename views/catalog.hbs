<div class="container">
  <header>
    <ul>
      <li><img class="logo" src="/static/images/logo.png" /></li>
      <li><a href="/">Browse</a></li>
      <li><a href="/create/cube">Add a Cube</a></li>
      <li><a href="/create/accessory">Add Accessory</a></li>
      <li><a href="/about">About</a></li><li><a href="/logout">Logout</a></li>
            <li><a href="/login">Login</a></li>
            <li><a href="/register">Register</a></li>
    </ul>
  </header>
  <main>
    <h1>Browser</h1>
    <form action="/" method="post">
      <input type="text" class="search" name="search" placeholder="Search..." />
      <input
        type="number"
        name="from"
        class="difficultyFrom"
        placeholder="Difficulty from..."
      />
      <span>-</span>
      <input
        type="number"
        name="to"
        class="difficultyTo"
        placeholder="Difficulty to..."
      />
      <input type="submit" value="search" />
    </form>
    {{#each cubes}}
    {{> cubeCard}}
    {{/each}}
    

  </main>
</div>